  "bugs": {
    "url": "https://github.com/adobe/aem-boilerplate/issues"
  },
  "homepage": "https://github.com/adobe/aem-boilerplate#readme",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "svelte": "^4.2.19",
    "svelte-check": "^4.0.5",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
